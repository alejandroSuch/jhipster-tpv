<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="1" author="alejandro">
        <loadData encoding="UTF-8"
                  file="config/liquibase/vats.csv"
                  separator=";"
                  tableName="vat">
            <column name="id" type="numeric"/>
            <column name="code"/>
            <column name="description"/>
            <column name="value" type="numeric"/>
            <column name="active" type="boolean"/>
            <column name="active_from" type="date"/>
            <column name="active_to" type="date"/>
        </loadData>
    </changeSet>

    <changeSet id="2" author="alejandro">
        <loadData encoding="UTF-8"
                  file="config/liquibase/categories.csv"
                  separator=";"
                  tableName="category">
            <column name="id" type="numeric"/>
            <column name="name"/>
            <column name="description"/>
            <column name="vat_id" type="numeric"/>
            <column name="parent_id" type="numeric"/>
        </loadData>
    </changeSet>

    <changeSet id="3" author="alejandro">
        <loadData encoding="UTF-8"
                  file="config/liquibase/prices.csv"
                  separator=";"
                  tableName="price">
            <column name="id" type="numeric"/>
            <column name="value" type="numeric"/>
            <column name="active" type="boolean"/>
            <column name="active_from" type="date"/>
        </loadData>
    </changeSet>

    <changeSet id="4" author="alejandro">
        <loadData encoding="UTF-8"
                  file="config/liquibase/products.csv"
                  separator=";"
                  tableName="product">
            <column name="id" type="numeric"/>
            <column name="active" type="boolean"/>
            <column name="active_from" type="date"/>
            <column name="active_to" type="date"/>
            <column name="code"/>
            <column name="name"/>
            <column name="description"/>
            <column name="price_id" type="numeric"/>
            <column name="category_id" type="numeric"/>
        </loadData>
    </changeSet>

    <changeSet id="5" author="alejandro">
        <loadData encoding="UTF-8"
                  file="config/liquibase/discounts.csv"
                  separator=";"
                  tableName="discount">
            <column name="id" type="numeric"/>
            <column name="code"/>
            <column name="description"/>
            <column name="value" type="numeric"/>
            <column name="units" type="numeric"/>
            <column name="active_from" type="date"/>
            <column name="active_to" type="date"/>
        </loadData>
    </changeSet>

    <changeSet id="6" author="alejandro">
        <loadData encoding="UTF-8"
                  file="config/liquibase/products_discounts.csv"
                  separator=";"
                  tableName="product_discount">
            <column name="discounts_id" type="numeric"/>
            <column name="products_id" type="numeric"/>
        </loadData>
    </changeSet>

</databaseChangeLog>
